{
  "name": "wizardcoder-vsc",
  "displayName": "wizardcoder-vsc",
  "publisher": "mzbac",
  "icon": "images/logo.png",
  "description": "wizardcode extension",
  "repository": {
    "type": "git",
    "url": "git@github.com:mzbac/wizardCoder-vsc.git"
  },
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "wizardCoder-vsc",
          "title": "Wizard Coder ",
          "icon": "$(comment-discussion)"
        }
      ]
    },
    "views": {
      "wizardCoder-vsc": [
        {
          "type": "webview",
          "id": "wizardCoder-vsc.view",
          "name": "Chat with Wizard Coder"
        }
      ]
    },
    "commands": [
      {
        "command": "wizardCoder-vsc.toggleActivate",
        "title": "toggle wizardCoder activation"
      },
      {
        "command": "wizardCoder-vsc.chat",
        "title": "Chat with Wizard Coder"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "wizardCoder-vsc.chat",
          "when": "editorTextFocus",
          "group": "wizardCoder-vsc"
        }
      ]
    },
    "keybindings": [
      {
        "command": "wizardCoder-vsc.toggleActivate",
        "key": "shift+ctrl+'",
        "mac": "shift+cmd+'"
      }
    ],
    "configuration": {
      "title": "Wizard Coder",
      "properties": {
        "wizardCoder.apiEndpoint": {
          "type": "string",
          "default": "http://60.16.6.168:15889/model/chat_stream",
          "description": "API endpoint for the Wizard Coder extension."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "node-fetch": "^3.3.1",
    "request": "^2.88.2"
  }
}
